{
    "name": "martynchuk/yii2-editable-cell",
    "description": "Yii2 GridView Editable Cell - editable cells in GridView",
    "type": "yii2-extension",
    "keywords": [
        "yii2",
        "extension",
        "gridview",
        "editable",
        "cell"
    ],
    "license": "MIT",
    "homepage": "https://github.com/martynchuk/yii2-editable-cell",
    "support": {
        "issues": "https://github.com/martynchuk/yii2-editable-cell/issues",
        "source": "https://github.com/martynchuk/yii2-editable-cell"
    },
    "authors": [
        {
            "name": "Yurii Martynchuk",
            "email": "yura@martynchuk.com"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "yiisoft/yii2": "~2.0.0"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "martynchuk\\editablecell\\": "src/"
        }
    },
    "extra": {
        "bootstrap": "martynchuk\\editablecell\\Bootstrap"
    }
}

